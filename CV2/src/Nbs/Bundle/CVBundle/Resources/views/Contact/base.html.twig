{% extends "OryzoneBoilerplateBundle::html5.html.twig" %}
{% block head_css %}
{{ parent() }}
<!-- Latest compiled and minified CSS -->
<link rel="stylesheet" href="{% block body_css_bootstrap_onlineSrc %}//netdna.bootstrapcdn.com/bootstrap/3.0.3/css/bootstrap.min.css{% endblock %}" id="body_css_bootstrap">
{# {% stylesheets '@FabienCrassatCurriculumVitaeBundle/Resources/public/css/OryzoneBoilerplate.template.css' %}
<link rel="stylesheet" href="{{ asset_url }}">
{% endstylesheets %} #}
{% endblock %}
{% block body_js %}
{{ parent() }}
<!-- Latest compiled and minified JavaScript -->
<script src="{% block body_js_bootstrap_onlineSrc %}//netdna.bootstrapcdn.com/bootstrap/3.0.3/js/bootstrap.min.js{% endblock %}"></script>
<script>
if ($.fn.button == undefined) {
    document.write('<script type="text/javascript" src="{% block body_js_bootstrap_offlineSrc %}{% javascripts '@FabienCrassatCurriculumVitaeBundle/Resources/offline/js/bootstrap.min.js' %}{{ asset_url }}{% endjavascripts %}{% endblock %}"><\/script>');
    document.write('<link rel="stylesheet" href="{% block body_css_bootstrap_offlineSrc %}{% stylesheets '@FabienCrassatCurriculumVitaeBundle/Resources/offline/css/bootstrap.min.css' %}{{ asset_url }}{% endstylesheets %}{% endblock %}" id="body_css_bootstrap_offlineSrc">');
    $('document').ready(function(){
        $('link#body_css_bootstrap_offlineSrc').appendTo('link#body_css_bootstrap')
    })
};
</script>
{% endblock %}
